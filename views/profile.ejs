<!DOCTYPE html>
<html>
  <head>
    <title><%=name%></title>
    <link rel="stylesheet" href="/assets/test.css" type="text/css">
  </head>
  <body class="profile-wrap">
    <% include partials/nav.ejs %>
    <h1>Profile of <%=name%></h1>
    <p><strong>Age : </strong><%=data.age%></p>
    <p><strong>Job : </strong><%=data.job%></p>
    <p>Hobbies</p>
    <ul>
      <% data.hobbies.forEach(function(item){ %>
        <li><%= item %></li>
      <% }) %>
    </ul>
  </body>
</html>
